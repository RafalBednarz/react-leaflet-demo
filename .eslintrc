{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": [1, 2, { "SwitchCase": 1 }],
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "jsx": true,
    "semi": [2, "never"],
    "ecmaFeatures": {
      "modules": true
    }
  }
}
